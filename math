p
f

a
d

q=1-p
z={-a,d,a}
hwe={q^2,2*p*q,p^2}
idev={p*q*f,-2*p*q*f,p*q*f}
freq=Simplify[hwe+idev]

muz=Simplify[z.freq]
zdev=Simplify[z-muz]
alpha0=a+d*(q-p)
alpha1=-d*(q-p)

bx={-2*alpha0*q,alpha0*(q-p),2*alpha0*p}
dx={}
ix={-2*alpha1*q*(2*fx)/(fx+1),alpha1*(q-p)*(2*fx)/(fx+1),2*alpha1*p*(2*fx)/(fx+1)}

by={-2*alpha0*q,alpha0*(q-p),2*alpha0*p}
dy={,,}
iy={-2*alpha1*q*(2*fy)/(fy+1),alpha1*(q-p)*(2*fy)/(fy+1),2*alpha1*p*(2*fy)/(fy+1)}

AxA=
DxD=()
IxI=()
